<file>

  <!-- FILAMENT VERTICES -->

  <mb_slice> <!-- A filament subset defined by the next properties -->
    <!-- Segmentation definition -->
    <name>mb_cont</name> <!-- name -->
    <side>2</side> <!-- segmentation label, value of vertex property mb_seg -->
    <cont>-</cont> <!-- if '+' then picking point correspond with the precise coordinate where a filament intersect a membrane,
                        otherwise all vertices within a filaments a are chosen as picking points-->
    <!-- Geometrical restictions -->
    <eu_dst id="low">-inf</eu_dst> <!-- Minimum Euclidean distance to membrane -->
    <eu_dst id="high">inf</eu_dst> <!-- Maximum Euclidean distance to membrane -->
    <eu_dst id="sign">+</eu_dst> <!-- if + then [min, max], otherwise [-inf,min]U[max,+inf] -->
    <geo_dst id="low">-inf</geo_dst> <!-- Minimum Geodesic distance to membrane -->
    <geo_dst id="high">inf</geo_dst> <!-- Maximum Geodesic distance to membrane -->
    <geo_dst id="sign">+</geo_dst> <!-- if + then [min, max], otherwise [-inf,min]U[max,+inf] -->
    <geo_len id="low">-inf</geo_len> <!-- Minimum Geodesic length to membrane -->
    <geo_len id="high">inf</geo_len> <!-- Maximum Geodesic length to membrane -->
    <geo_len id="sign">+</geo_len> <!-- if + then [min, max], otherwise [-inf,min]U[max,+inf] -->
    <sin id="low">-inf</sin> <!-- Minimum filament sinuosity -->
    <sin id="high">inf</sin> <!-- Maximum filament sinuosity -->
    <sin id="sign">+</sin> <!-- if + then [min, max], otherwise [-inf,min]U[max,+inf] -->
    <!-- Not used -->
    <clst_nv id="low">-inf</clst_nv>
    <clst_nv id="high">inf</clst_nv>
    <clst_nv id="sign">+</clst_nv>
  </mb_slice>
 
  
</file> 
